<Window x:Class="SendScheduleFinals.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SendScheduleFinals"
        mc:Ignorable="d"
        Title="MainWindow" Height="335" Width="615">
    <Grid>
        <Label Content="SEND SCHEDULE" FontSize="24" FontStyle="Italic" FontWeight="DemiBold" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="tbx_email" HorizontalAlignment="Left" TextWrapping="Wrap" Text="" FontSize="18" VerticalAlignment="Top" Width="368" Height="32" Margin="136,129,0,0"/>
        <Label Content="EMAIL:" FontWeight="DemiBold" HorizontalAlignment="Left" Margin="83,132,0,0" VerticalAlignment="Top"/>
        <Button Content="SEND SCHEDULE" FontStyle="Italic" FontWeight="DemiBold" HorizontalAlignment="Left" Margin="382,178,0,0" VerticalAlignment="Top" Height="26" Width="122" Click="Button_Click"/>
        <Button Content="BACK" FontStyle="Italic" FontWeight="DemiBold" HorizontalAlignment="Left" Margin="10,259,0,0" VerticalAlignment="Top" Height="26" Width="97"/>

    </Grid>
</Window>
